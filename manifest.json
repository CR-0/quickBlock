{
  "name": "Quick Block",
  "version": "1.0",
  "description": "Think about it!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*","https://*.youtube.com/*"],
      "js": ["script.js"]
    }
  ],
  "browser_action": {
    "name": "Block"
  },
  "manifest_version": 2
}